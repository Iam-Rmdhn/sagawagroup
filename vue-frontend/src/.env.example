# Environment Variables untuk YouTube API
# Copy file ini menjadi .env.local dan isi dengan nilai yang sebenarnya

# YouTube Data API v3 Key
# Dapatkan dari: https://console.cloud.google.com/
YOUTUBE_API_KEY=your_youtube_api_key_here

# YouTube Channel ID
# Format: UC... (contoh: UCxxxxxxxxxxxxxxxxxxxxx)
YOUTUBE_CHANNEL_ID=your_channel_id_here

# YouTube Playlist ID (opsional)
# Hanya isi jika ingin menampilkan playlist tertentu instead of channel
YOUTUBE_PLAYLIST_ID=

# Maksimal video yang ditampilkan (1-50)
YOUTUBE_MAX_RESULTS=9

# Urutan video: date, relevance, rating, title, viewCount
YOUTUBE_ORDER_BY=date

# Filter video setelah tanggal tertentu (format ISO 8601)
# Contoh: 2024-01-01T00:00:00Z
# Kosongkan jika tidak ingin filter tanggal
YOUTUBE_PUBLISHED_AFTER=

# Cache timeout dalam milidetik (default: 5 menit = 300000)
YOUTUBE_CACHE_TIMEOUT=300000

# Development mode (true/false)
# Jika true, akan menampilkan log debug dan fungsi clearYouTubeCache()
YOUTUBE_DEV_MODE=false